{
  "bot_name": "SHADOW_D_FREQAI",
  "api_server": { "listen_port": 8083 },
  "dry_run": true,
  "db_url": "sqlite:////freqtrade/user_data/instances/d/trades.sqlite",
  "logfile": "/freqtrade/user_data/instances/d/logs/freqtrade.d.log",
  "strategy": "FreqaiExampleStrategy",
  "freqai": {
    "enabled": true,
    "purge_old_models": 2,
    "train_period_days": 60,
    "backtest_period_days": 7,
    "identifier": "hl-btc-4h",
    "feature_parameters": {
      "include_timeframes": ["1h"],
      "include_corr_pairlist": ["BTC/USDC:USDC"],
      "label_period_candles": 20,
      "include_shifted_candles": 2,
      "indicator_periods_candles": [10, 20],
      "use_SVM_to_remove_outliers": true,
      "principal_component_analysis": false,
      "plot_feature_importances": 0
    },
    "live_retrain_hours": 1,
    "data_split_parameters": { "test_size": 0.33, "random_state": 1 },
    "model_training_parameters": {}
  }
}
